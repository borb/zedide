{
  "name": "web-z80-ide",
  "version": "0.0.1",
  "description": "an ide for writing, running and debugging z80 asm on the web",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build:dev": "webpack --debug",
    "start:dev": "webpack-dev-server",
    "opcodes:base": "z80_tables/translate_z80_tables.js z80_tables/opcodes_base.dat z80_tables/build/opcodes_base.js",
    "opcodes:cb": "z80_tables/translate_z80_tables.js z80_tables/opcodes_cb.dat z80_tables/build/opcodes_cb.js cb",
    "opcodes:dd": "z80_tables/translate_z80_tables.js z80_tables/opcodes_ddfd.dat z80_tables/build/opcodes_dd.js dd ix",
    "opcodes:ed": "z80_tables/translate_z80_tables.js z80_tables/opcodes_ed.dat z80_tables/build/opcodes_ed.js ed",
    "opcodes:fd": "z80_tables/translate_z80_tables.js z80_tables/opcodes_ddfd.dat z80_tables/build/opcodes_fd.js fd iy",
    "opcodes:ddcb": "z80_tables/translate_z80_tables.js z80_tables/opcodes_ddfdcb.dat z80_tables/build/opcodes_ddcb.js ddcb ix",
    "opcodes:fdcb": "z80_tables/translate_z80_tables.js z80_tables/opcodes_ddfdcb.dat z80_tables/build/opcodes_fdcb.js fdcb iy",
    "generate:opcodes": "npm run-script opcodes:base && npm run-script opcodes:cb && npm run-script opcodes:dd && npm run-script opcodes:ed && npm run-script opcodes:fd && npm run-script opcodes:ddcb && npm run-script opcodes:fdcb"
  },
  "repository": {
    "type": "git",
    "url": "github.com:borb/web-z80-ide.git"
  },
  "author": "rob andrews <rob@aphlor.org>",
  "license": "CC-BY-SA-4.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "asm80": "^1.11.14",
    "codemirror": "^5.56.0",
    "css-loader": "^4.0.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.0"
  },
  "devDependencies": {
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
